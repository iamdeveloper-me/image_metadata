<div class="container-fluid pageheading">
  <div  class="container"><h3>Sign up</h3></div>
</div>
<div  class="container-fluid whitebox">
  <div class="container padd15px martop15px">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%#= devise_error_messages! %>

      <div class="form-group row">
        <div class="col-md-3">
          <%= f.label :first_name %>
        </div>
        <div class="col-md-6">
          <%= f.text_field :first_name,
            autofocus: true,
            autocomplete: "off",
            class: 'form-control'
          %>
          <%= render partial: 'layouts/form_field_error', locals: { field_error: f.object.errors.messages[:first_name] } %>
          
        </div>
      </div>

      <div class="form-group row">
        <div class="col-md-3">
          <%= f.label :last_name %>
        </div>
        <div class="col-md-6">
          <%= f.text_field :last_name,
            autofocus: true,
            autocomplete: "off",
            class: 'form-control'
          %>
          <%= render partial: 'layouts/form_field_error', locals: { field_error: f.object.errors.messages[:last_name] } %>
        </div>
      </div>

      <div class="form-group row">
        <div class="col-md-3">
          <%= f.label :email %>
        </div>
        <div class="col-md-6">
          <%= f.email_field :email,
            autofocus: true,
            autocomplete: "off",
            class: 'form-control'
          %>
          <%= render partial: 'layouts/form_field_error', locals: { field_error: f.object.errors.messages[:email] } %>
        </div>
      </div>

      <div class="form-group row">
        <div class="col-md-3">
          <%= f.label :password %>
          
        </div>
        <div class="col-md-6">
          <%= f.password_field :password,
            autocomplete: "off",
            class: 'form-control'
          %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %>
          <br/>
          <%= render partial: 'layouts/form_field_error', locals: { field_error: f.object.errors.messages[:password] } %>
        </div>
      </div>

      <div class="form-group row">
        <div class="col-md-3">
          <%= f.label :password_confirmation %>
        </div>
        <div class="col-md-6">
          <%= f.password_field :password_confirmation,
            autocomplete: "off",
            class: 'form-control'
          %>
          <%= render partial: 'layouts/form_field_error', locals: { field_error: f.object.errors.messages[:password_confirmation] } %>
        </div>
      </div>
 <div class="form-group row">
        <div class="col-md-3"></div>
      <div class="actions col-md-6 text-left">
        <%= f.submit "Sign up", class: "btn btn-success" %>
      </div>
    </div>
    <% end %>
  </div>
</div>
