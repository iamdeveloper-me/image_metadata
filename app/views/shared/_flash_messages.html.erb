<% if notice %>
  <p class="alert alert-warning marbot0px"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger marbot0px"><%= alert %></p>
<% end %>
<% if flash['success'] %>
  <p class="alert alert-success marbot0px"><%= flash['success'] %></p>
<% end %>
<% if flash['error'] %>
  <p class="alert alert-danger marbot0px"><%= flash['error'] %></p>
<% end %>
